{
  "name": "musicplayer",
  "version": "1.0.0",
  "description": "Javascript music player",
  "main": "gulpfile.js",
  "type": "module",
  "scripts": {
    "pushall": "git add --all && git commit -m 'update' && git push -u origin v2",
    "dev": "node server.js",
    "clear": "docker rmi musicplayer && docker rmi brunoperry/musicplayer:latest",
    "build_frontend": "gulp --gulpfile frontend/deploy/gulpfile.js && cp frontend/app_logo.svg public/ && cp frontend/manifest.json public/ && cp frontend/service-worker.js public/ && cp -R frontend/images/ public/images/",
    "production": "pm2 start server.js && pm2 log 0",
    "build": "docker build -t musicplayer . && docker tag musicplayer brunoperry/musicplayer:latest",
    "deploy": "docker login && docker push brunoperry/musicplayer:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunoperry/musicplayer.git"
  },
  "keywords": [
    "music",
    "player",
    "web",
    "js",
    "es6"
  ],
  "author": "brunoperry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brunoperry/musicplayer/issues"
  },
  "homepage": "https://github.com/brunoperry/musicplayer#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "^9.16.0"
  },
  "devDependencies": {
    "del": "^7.0.0",
    "express": "^4.18.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-each": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-source-injector": "^1.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-terser": "^2.1.0"
  }
}
